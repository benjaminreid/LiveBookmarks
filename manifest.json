{
  "name": "LiveBookmarks",
  "version": "1.0.2",
  "manifest_version": 1,
  "description": "Live search through all your bookmarks!",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "bookmarks.html"
  },
  "permissions": [
    "bookmarks",
    "https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"
  ],
  "icons": { "16": "icon16.png",
              "48": "icon48.png",
              "128": "icon128.png" },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://ajax.googleapis.com; object-src 'self'"
}